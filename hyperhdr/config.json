{
  "name": "HyperHDR",
  "version": "auto",
  "slug": "hyperhdr",
  "description": "HyperHDR Ambilight prebuilt for ARM64",
  "url": "htpps://www.hyperhdr.eu/",
  "startup": "application",
  "apparmor": false,
  "boot": "auto",
  "arch": ["aarch64"],
  "init": false,
  "map": ["config:rw"],
  "image": "obaida/hyperhdr-arm64:latest",
  "ports": {
    "8090/tcp": 8090,
    "8092/tcp": null,
    "19333/tcp": 19333,
    "19400/tcp": 19400,
    "19444/tcp": 19444,
    "19445/tcp": 19445
  },
  "ports_description": {
    "8090/tcp": "Web Admin",
    "8092/tcp": "SSL Web Admin",
    "19333/tcp": "Boblight Server",
    "19400/tcp": "Google Flatbuffers Receiver",
    "19444/tcp": "JSON-RPC Remote Control",
    "19445/tcp": "Protocol Buffers Server"
  },
  "options": {},
  "schema": {},
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "uart": true,
  "usb": true,
  "video": true,
  "gpio": true,
  "devices": [
    "/dev/spidev0.0",
    "/dev/vchiq",
    "/dev/amvideo",
    "/dev/fb0",
    "/dev/mem"
  ],
  "webui": "http://[HOST]:[PORT:8090]",
  "ingress": false,
  "panel_icon": "mdi:television-ambient-light",
  "init": false
}
